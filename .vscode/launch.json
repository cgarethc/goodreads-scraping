{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
      


        {
            "type": "node",
            "request": "launch",
            "name": "Run test script",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/testmanual.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Run testbook",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/testbook.js",
            "outputCapture": "std"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Scrape award",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/whatcaniborrow.js",
            "args": ["-l", "https://www.goodreads.com/list/show/177853.New_York_Times_Ten_Best_Books_Fiction"],
            "outputCapture": "std"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Just one list",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/populatefirestore.js",
            "args": ["-i", "justonelist.json"],
            "outputCapture": "std"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "User toreads",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/populatefirestore.js",
            "args": ["-u"],
            "outputCapture": "std"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Run stats",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/showmestats.js",
            "args": ["-u", "4622353", "-d", "-m", "stats"],
            "outputCapture": "std"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Run a generate stats -d",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/generatestats.js",
            "args": ["-u", "3408361", "-d", "-m", "stats"],
            "outputCapture": "std"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Firebase award",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/populatefirestore.js",             
            "args": ["-f", "Kirkus Prize for Fiction", "-a", "https://www.goodreads.com/award/show/20886-kirkus-prize",
                "-i", "kirkus-fiction" ,"-n", "Kirkus Prize for Fiction"],
            "outputCapture": "std"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Scrape award",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/whatcaniborrow.js",
            "args": ["-a", "https://www.goodreads.com/award/show/9-hugo-award"],
            "outputCapture": "std"
        },
        {
          "name": "Debug Jest Tests",
          "type": "node",
          "request": "launch",
          "runtimeArgs": [
            "--inspect-brk",
            "${workspaceRoot}/node_modules/.bin/jest",
            "--runInBand"
          ],
          "console": "integratedTerminal",
          "internalConsoleOptions": "neverOpen",
          "port": 9229
        }
    ]
}